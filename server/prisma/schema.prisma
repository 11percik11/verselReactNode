// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL") // будет подтягивать строку из .env
}

generator client {
  provider = "prisma-client-js"
}

// Пример модели
model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}
